@page "/head-simulation"
@inject WebviewAppTest.AppState AppState

<canvas id="game-window" @onkeydown="HandleKeyDown"></canvas>

<style>
    html, body {
        overflow: hidden;
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;
    }

    #game-window {
        width: 100%;
        height: 100%;
        touch-action: none;
    }
</style>

